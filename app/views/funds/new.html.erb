<% if @fund.errors.any? %>
  <div id="errors">
    <h4>
      Error:
    </h4>

    <ul>
    <% @fund.errors.full_messages.each do |msg| %>
      <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>

<h1>Create A New Fund:</h1>

<%=render partial: 'form', locals: {user: @user, fund: @fund}%>
